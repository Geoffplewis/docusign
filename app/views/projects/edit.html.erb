<h1> EDIT ME! <%= @project.name %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@project) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      
      <%= f.file_field :documents, multiple: true %>
      

      <%= f.submit "Update", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
