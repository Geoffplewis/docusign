<h1>Add new document to project</h1>

<p>Upload new document:</p>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for([@project, @document]) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      
      <%= f.file_field :attachment %>
      

      <%= f.submit "Add!", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
